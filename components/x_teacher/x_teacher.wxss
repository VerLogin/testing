.TeacherItem{
  height: 130px;
  width: 100px;
  border-radius: 10%;
  background-color: blueviolet;
}
.TeacherItem image{
  height: 100px;
  width: 100px;
  border-radius: 50%;
  margin-top: 5px;
  border: 5px dashed red;
  box-sizing: border-box;
}
.TeacherItem text{
  display: block;
  width: 100%;
  text-align: center;
  color: white;
}